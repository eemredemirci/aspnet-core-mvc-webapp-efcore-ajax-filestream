@model LoginDTO
  
    @if (Model.UserName!="")  
    {  
        <li class="nav-item">  
            <a class="nav-link text-dark" title="Manage">Hello @Model.UserName</a>  
        </li>  
        <li class="nav-item">  
            <a class="nav-link btn btn-link text-dark" asp-controller="account" asp-action="Logout">Logout</a>  
        </li>  
    }  
    else  
    {  
        <li class="nav-item">  
            <a class="nav-link text-dark" asp-controller="" asp-action="">Register(temsili)</a>  
        </li>  
        <li class="nav-item">  
            <a class="nav-link text-dark" asp-controller="Home" asp-action="Index">Login</a>  
        </li>  
    }  
